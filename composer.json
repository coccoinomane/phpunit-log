{
  "name": "coccoinomane/phpunit-log",
  "description": "Log from within your PHPUnit tests.",
  "keywords": [
    "tests",
    "php",
    "phpunit",
    "trait"
  ],
  "type": "library",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/coccoinomane/phpunit-log",
  "scripts": {
    "test": [
      "phpunit --testdox"
    ],
    "prettier": "prettier -w **/*.{php,json,js,css,scss,html}"
  },
  "autoload": {
    "psr-4": {
      "PHPUnitLog\\": "src/",
      "PHPUnitLog\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/extension-installer": "^1.1"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
